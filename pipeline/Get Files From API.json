								}
							},
							"ThreadID": {
								"type": "Int32",
								"value": {
									"value": "@int(pipeline().parameters.ThreadID)",
									"type": "Expression"
								}
							}
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "OPS_LIST",
						"type": "DatasetReference"
					}
				}
			}
		],
		"parameters": {
			"MasterPackageID": {
				"type": "string",
				"defaultValue": "-1"
			},
			"ThreadID": {
				"type": "string",
				"defaultValue": "1"
			}
		},
		"folder": {
			"name": "AGGDW"
